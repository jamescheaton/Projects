<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<?import java.lang.Integer?>
<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.SearchAnimalsController">
   <children>
      <Label layoutX="35.0" layoutY="28.0" text="Atlanta Zoo" />
      <Label layoutX="116.0" layoutY="28.0" text="Animals" />
      <Label layoutX="35.0" layoutY="71.0" text="Name" />
      <Label layoutX="37.0" layoutY="129.0" text="Species" />
      <Label layoutX="296.0" layoutY="20.0" text="Exhibit" />
      <Label layoutX="304.0" layoutY="71.0" text="Age" />
      <Label layoutX="301.0" layoutY="129.0" text="Type" />
      <TextField fx:id="nameField" layoutX="97.0" layoutY="67.0" prefHeight="25.0" prefWidth="106.0" />
      <TextField fx:id="speciesField" layoutX="97.0" layoutY="125.0" prefHeight="25.0" prefWidth="106.0" />
      <ChoiceBox fx:id="typeChoice" layoutX="349.0" layoutY="125.0" prefHeight="25.0" prefWidth="80.0">
          <items>
              <FXCollections fx:factory="observableArrayList">
                  <String fx:value="Mammal" />
                  <String fx:value="Bird" />
                  <String fx:value="Reptile" />
                  <String fx:value="Fish" />
                  <String fx:value="Invertebrate" />
                  <String fx:value="Amphibian" />
              </FXCollections>
          </items>
      </ChoiceBox>
      <Label layoutX="349.0" layoutY="45.0" text="Min" />
      <Label layoutX="443.0" layoutY="45.0" text="Max" />
      <Button layoutX="270.0" layoutY="175.0" mnemonicParsing="false" text="Search" onAction="#handleSearchButtonAction" />
      <TableView fx:id="table" layoutX="37.0" layoutY="210.0" prefHeight="163.0" prefWidth="533.0">
      </TableView>
      <Spinner fx:id="ageMinSpinner" min="0" initialValue="0" amountToStepBy="1" editable="true" layoutX="349.0" layoutY="67.0" prefHeight="25.0" prefWidth="80.0">
       <max>
           <Integer fx:constant="MAX_VALUE"/>
       </max>
      </Spinner>
      <Spinner fx:id="ageMaxSpinner" min="0" initialValue="100" amountToStepBy="1" editable="true" layoutX="443.0" layoutY="67.0" prefHeight="25.0" prefWidth="80.0">
          <max>
              <Integer fx:constant="MAX_VALUE"/>
          </max>
      </Spinner>
      <TextField fx:id="exhibitField" layoutX="349.0" layoutY="16.0" prefWidth="150.0" />
      <Button layoutX="518.0" layoutY="175.0" mnemonicParsing="false" onAction="#handleHomeButtonAction" text="Home" />
   </children>
</AnchorPane>
