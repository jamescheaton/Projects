<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.text.Text?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.StaffPageController">
    <padding><Insets bottom="10" left="25" right="25" top="25" /></padding>

    <Text text="Atlanta Zoo" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="0" />

    <Button onAction="#handleViewShowsButtonAction" text="View Shows" GridPane.columnIndex="0" GridPane.rowIndex="1" />

    <Button onAction="#handleSearchAnimalsButtonAction" text="Search for Animals" GridPane.columnIndex="1" GridPane.rowIndex="1" />

    <Button onAction="#handleLogoutButtonAction" text="Logout" GridPane.columnIndex="2" GridPane.rowIndex="1" />



</GridPane>
